{
  "routes": [
    {
      "src": "/map/api",
      "methods": ["GET", "POST"],
      "dest": "https://maps.googleapis.com/map/api",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ],

  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
}
